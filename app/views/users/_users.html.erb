<% if users.any? %>
  <ul class="media-list">
    <% users.each do |user| %>
      <div class="media-body">
        <div>
          id: <%= user.id %>,&nbsp;
        </div>
        <div>
          <%= user.name %>
        </div>
        <div>
          <p><%= link_to 'View profile', user_path(user) %></p>
        </div>
      </div>
    <% end %>
    
  </ul>
  <%= paginate users %>
  
<% end %>
